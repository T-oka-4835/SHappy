<h2>自分をフォロー中のUser</h2>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>写真</th>
      <th>ニックネーム</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% if @users.exists? %>
      <% @users.each do |user| %>
        <tr>
          <td></td>
          <td><%= link_to user.name, user_path(user) %></td>
          <td></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>